package search;

public class p2 {
	
	//binary search
	
	public static int binarySearch(int[]arr, int key, int n) {
		
		int left = 0;
		int right =n;
		
		while (left<=right) {
			int mid = (left+right)/2;
			
			if(key<arr[mid]) {
				right = mid-1;
				}
			else if(key>arr[mid]) {
				left = mid+1;
			}else {
				return mid;
			}
			
		}
		return -1;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
